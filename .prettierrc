{
    "bracketSpacing": true,
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "all",
    "tabWidth": 2,
    "semi": true,
    "useTabs": false,
    "endOfLine": "auto",
    "importOrder": [
        "^(^react$|@react|react|^redux$)",
        "^@mui/(.*)$",
        "<THIRD_PARTY_MODULES>",
        "^@/(.*)$",
        "^[./]"
      ],
      "importOrderGroupNamespaceSpecifiers": true,
      "importOrderSeparation": true,
      "importOrderSortSpecifiers": true
}